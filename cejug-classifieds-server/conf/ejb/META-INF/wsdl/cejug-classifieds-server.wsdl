<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions name="cejug-classifieds"
	targetNamespace="http://cejug-classifieds.dev.java.net"
	xmlns:contract="http://cejug-classifieds.dev.java.net"
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap12/"
	xmlns:contract-business="http://cejug-classifieds.dev.java.net/business"
	xmlns:contract-admin="http://cejug-classifieds.dev.java.net/admin">

	<wsdl:import namespace="http://cejug-classifieds.dev.java.net/admin"
		location="cejug-classifieds-admin.wsdl" />
	<wsdl:import
		namespace="http://cejug-classifieds.dev.java.net/business"
		location="cejug-classifieds-business.wsdl" />

	<wsdl:service name="CejugClassifiedsService">
		<wsdl:port name="CejugClassifiedsAdmin"
			binding="contract-admin:CejugClassifiedsAdminBinding">
			<soap:address
				location="http://localhost:8080/cejug-classifieds-ws/admin" />
		</wsdl:port>
		<wsdl:port name="CejugClassifiedsBusiness"
			binding="contract-business:CejugClassifiedsBusinessBinding">
			<soap:address
				location="http://localhost:8080/cejug-classifieds-ws/business" />
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>
