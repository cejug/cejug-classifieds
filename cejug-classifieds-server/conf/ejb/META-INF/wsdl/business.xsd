	<xsd:complexType name="Section">
		<xsd:sequence>
			<xsd:element name="names" type="xsd:string" />
			<xsd:element name="description" type="xsd:string" />
		</xsd:sequence>
	</xsd:complexType>

	<xsd:element name="Advertisement">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="id" type="xsd:int" />
				<xsd:element name="advertiser" type="business:Customer" />
				<xsd:element name="headline" type="xsd:string" />
				<xsd:element name="short.description" type="xsd:string" />
				<xsd:element name="full.text" type="xsd:string" />
				<xsd:element name="status" type="xsd:int" />
				<xsd:element name="keywords" type="xsd:string" />
				<xsd:element name="publishing.period"
					type="business:Period" />
				<xsd:element name="attachment.id" type="xsd:int"
					minOccurs="0" maxOccurs="unbounded" />
				<xsd:element name="category.id" type="xsd:int" />
				<xsd:element name="locale" type="business:Locale" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>


	<xsd:element name="PublishingHeader">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="customer.login" type="xsd:NMTOKEN" />
				<xsd:element name="customer.domain.id" type="xsd:int" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:complexType name="Customer">
		<xsd:sequence>
			<xsd:element name="login" type="xsd:string" />
			<xsd:element name="domain" type="xsd:string" />
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="Locale">
		<xsd:sequence>
			<xsd:element name="language" type="xsd:string" />
			<xsd:element name="country" type="xsd:string" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="RssCollection">
		<xsd:complexType>
			<xsd:sequence minOccurs="1" maxOccurs="unbounded">
				<xsd:element name="rss.collection" type="rss20:channel" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="AdvertisementCollection">
		<xsd:complexType>
			<xsd:sequence minOccurs="1" maxOccurs="unbounded">
				<xsd:element ref="business:Advertisement" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="AdvertisementCollectionFilter">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="category" type="xsd:NMTOKEN" />
				<xsd:element name="maximum.number.of.records"
					type="xsd:int" default="30" />
				<xsd:element name="period" type="business:Period"
					nillable="true" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:complexType name="Period">
		<xsd:sequence>
			<xsd:element name="start" type="xsd:dateTime" />
			<xsd:element name="finish" type="xsd:dateTime" />
		</xsd:sequence>
	</xsd:complexType>

	<xsd:element name="AtomCollection">
		<xsd:complexType>
			<xsd:sequence minOccurs="1" maxOccurs="unbounded">
				<xsd:element name="atom.collection"
					type="atom10:feedType" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="AtomFilterCollection">
		<xsd:complexType>
			<xsd:sequence minOccurs="1" maxOccurs="unbounded">
				<xsd:element name="filter.collection"
					type="business:SyndicationFilter" nillable="true" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:complexType name="SyndicationFilter">
		<xsd:sequence>
			<xsd:element name="upper.bound" type="xsd:int"
				nillable="true" />
			<xsd:element name="date.initial" type="xsd:dateTime" />
			<xsd:element name="date.final" type="xsd:dateTime" />
			<xsd:element name="section" type="xsd:string" />
			<xsd:element name="keywords" type="xsd:string" minOccurs="0"
				maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>

	<xsd:element name="RssFilterCollection">
		<xsd:complexType>
			<xsd:sequence minOccurs="1" maxOccurs="unbounded">
				<xsd:element name="filter.collection"
					type="business:SyndicationFilter" nillable="true" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="SpamReport">
		<xsd:complexType>
			<xsd:sequence minOccurs="1" maxOccurs="1">
				<xsd:element name="advertisement.id" type="xsd:int" />
				<xsd:element name="reason" type="xsd:string" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>