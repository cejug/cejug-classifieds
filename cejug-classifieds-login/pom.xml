<?xml version="1.0"?>
<project>
	<parent>
		<artifactId>cejug-classifieds</artifactId>
		<groupId>net.java.dev.cejug.classifieds
		</groupId>
		<version>1.0</version>
	</parent>
	<modelVersion>4.0.0</modelVersion>
	<groupId>net.java.dev.cejug.classifieds.login
	</groupId>
	<artifactId>cejug-classifieds-login</artifactId>
	<name>cejug-classifieds-login</name>
	<version>1.0</version>
	<url>http://maven.apache.org</url>
	<build>
		<defaultGoal>package</defaultGoal>
		<directory>${basedir}/target</directory>
		<finalName>${artifactId}-${date}</finalName>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>2.0.2</version>
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<dependencies>
		<dependency>
			<groupId>javax.persistence</groupId>
			<artifactId>persistence-api</artifactId>
			<version>1.0</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>javax.ejb</groupId>
			<artifactId>ejb</artifactId>
			<version>3.0</version>
			<scope>provided</scope>
		</dependency>
	</dependencies>
	<scm>
		<connection>
			scm:svn:https://cejug-classifieds.dev.java.net/svn/cejug-classifieds/trunk
		</connection>
		<developerConnection>
			scm:svn:https://cejug-classifieds.dev.java.net/svn/cejug-classifieds/trunk/cejug-classifieds-login
		</developerConnection>
		<tag>HEAD</tag>
		<url>http://cejug-classifieds.dev.java.net
		</url>
	</scm>
	<reporting>
		<plugins>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>findbugs-maven-plugin</artifactId>
				<version>1.0-beta-1</version>
				<configuration>
					<effort>Max</effort>
					<threshold>Low</threshold>
					<!-- includeFilterFile>whizbang/lib-filter.xml</includeFilterFile-->
				</configuration>
			</plugin>
		</plugins>
	</reporting>
</project>
